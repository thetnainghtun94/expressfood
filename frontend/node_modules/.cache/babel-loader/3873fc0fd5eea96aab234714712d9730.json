C,GAAR,CAAY,KAAZ;AACF,OAbJ;AAeD,KAjBM;AAkBP,IAAA,SAlBO,qBAkBG,SAlBH,EAkBc;AAAA;;AACnB,UAAI,CAAC,KAAK,KAAV,EAAiB;AACf,QAAA,IAAI,CAAC;AACH,UAAA,IAAI,EAAE,sBADH;AAEH,UAAA,IAAI,EAAE;AAFH,SAAD,CAAJ;AAIA;AACF;;AACA,MAAA,KAAI,CACD,IADH,WACW,KAAK,OADhB,4BACyC,KAAK,KAD9C,GACuD;AACnD,QAAA,SAAS,EAAE,SADwC;AAEnD,QAAA,QAAQ,EAAE,KAAK;AAFoC,OADvD,EAKG,IALH,CAMI,UAAC,QAAD,EAAc;AACZ,YAAI,QAAQ,CAAC,MAAT,IAAmB,GAAvB,EAA4B;AAC1B,UAAA,IAAI,CAAC;AACH,YAAA,IAAI,EAAE,4BADH;AAEH,YAAA,IAAI,EAAE,SAFH;AAGH,YAAA,mBAAmB,EAAE;AAHlB,WAAD,CAAJ,CAD0B,CAM1B;;AACA,UAAA,MAAI,CAAC,KAAL,CAAW,WAAX;AACF;AACD,OAhBL,EAiBI,UAAC,KAAD,EAAW;AACT,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACF,OAnBJ;AAqBD,KA/CM;AAiDP,IAAA,aAjDO,2BAiDS;AAAA;;AACd,MAAA,KAAK,CAAC,GAAN,WAAa,KAAK,OAAlB,yBAAwC,KAAK,KAA7C,GAAsD,IAAtD,CACE,UAAC,QAAD,EAAc;AACZ,YAAI,QAAQ,CAAC,MAAT,KAAoB,GAAxB,EAA6B;AAC3B,UAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,OAAlB;AACF;AACD,OALH,EAME,UAAC,KAAD,EAAW;AACT,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACF,OARF;AAUD;AA5DM,GAbI;AA2Eb,EAAA,OA3Ea,qBA2EH;AAAA;;AACR,SAAK,EAAL,GAAU,KAAK,MAAL,CAAY,MAAZ,CAAmB,EAA7B;AACA,SAAK,OAAL,GAAe,KAAK,QAAL,CAAc,IAAd,CAAmB,UAAC,OAAD;AAAA,aAAa,OAAO,CAAC,EAAR,IAAc,MAAI,CAAC,EAAhC;AAAA,KAAnB,CAAf;AACA,SAAK,QAAL,GAAgB,KAAK,UAAL,CAAgB,IAAhB,CACd,UAAC,QAAD;AAAA,aAAc,QAAQ,CAAC,EAAT,IAAe,MAAI,CAAC,OAAL,CAAa,UAA1C;AAAA,KADc,CAAhB;AAGA,SAAK,KAAL,GAAa,YAAY,CAAC,OAAb,CAAqB,OAArB,CAAb;AACD;AAlFY,CAAf","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"row pt-5\">\r\n      <div class=\"col-md-1\"></div>\r\n      <div class=\"col-md-4 col-12\">\r\n        <img :src=\"product.imageURL\" :alt=\"product.name\" class=\"img-fluid\" />\r\n      </div>\r\n      <div class=\"col-md-6 col-12 pt-3 pt-md-0\">\r\n        <h4>{{ product.name }}</h4>\r\n        <h6 class=\"category font-italic\">{{ category.categoryName }}</h6>\r\n        <h6 class=\"font-weight-bold\">$ {{ product.price }}</h6>\r\n        <p>\r\n          {{ product.description }}\r\n        </p>\r\n\r\n        <div class=\"d-flex flex-row justify-content-between\">\r\n          <div class=\"input-group col-md-3 col-4 p-0\">\r\n            <div class=\"input-group-prepend\">\r\n              <span class=\"input-group-text\" id=\"basic-addon1\">Quantity</span>\r\n            </div>\r\n            <input class=\"form-control\" type=\"number\" v-bind:value=\"quantity\" />\r\n          </div>\r\n\r\n          <div class=\"input-group col-md-3 col-4 p-0\">\r\n            <button\r\n              type=\"button\"\r\n              id=\"add-to-cart-button\"\r\n              class=\"btn\"\r\n              @click=\"addToCart(this.id)\"\r\n            >\r\n              Add to Cart\r\n              <ion-icon name=\"cart-outline\" v-pre></ion-icon>\r\n            </button>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"features pt-3\">\r\n          <h5><strong>Features</strong></h5>\r\n          <ul>\r\n            <li>Lorem ipsum dolor sit amet consectetur adipisicing elit.</li>\r\n            <li>Officia quas, officiis eius magni error magnam voluptatem</li>\r\n            <li>nesciunt quod! Earum voluptatibus quaerat dolorem doloribus</li>\r\n            <li>molestias ipsum ab, ipsa consectetur laboriosam soluta et</li>\r\n            <li>ut doloremque dolore corrupti, architecto iusto beatae.</li>\r\n          </ul>\r\n        </div>\r\n\r\n        <button\r\n          id=\"wishlist-button\"\r\n          class=\"btn mr-3 p-1 py-0\"\r\n          :class=\"{ product_added_wishlist: isAddedToWishlist }\"\r\n          @click=\"addToWishList(this.id)\"\r\n        >\r\n          {{ wishlistString }}\r\n        </button>\r\n        <button\r\n          id=\"show-cart-button\"\r\n          type=\"button\"\r\n          class=\"btn mr-3 p-1 py-0\"\r\n          @click=\"listCartItems()\"\r\n        >\r\n          Show Cart\r\n\r\n          <ion-icon name=\"cart-outline\" v-pre></ion-icon>\r\n        </button>\r\n      </div>\r\n      <div class=\"col-md-1\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      product: {},\r\n      category: {},\r\n      id: null,\r\n      token: null,\r\n      isAddedToWishlist: false,\r\n      wishlistString: \"Add to wishlist\",\r\n      quantity: 1,\r\n    };\r\n  },\r\n  props: [\"baseURL\", \"products\", \"categories\"],\r\n  methods: {\r\n    addToWishList(productId) {\r\n      axios\r\n        .post(`${this.baseURL}wishlist/add?token=${this.token}`, {\r\n          id: productId,\r\n        })\r\n        .then(\r\n          (response) => {\r\n            if (response.status == 201) {\r\n              this.isAddedToWishlist = true;\r\n              this.wishlistString = \"Added to WishList\";\r\n            }\r\n          },\r\n          (error) => {\r\n            console.log(error);\r\n          }\r\n        );\r\n    },\r\n    addToCart(productId) {\r\n      if (!this.token) {\r\n        swal({\r\n          text: \"Please log in first!\",\r\n          icon: \"error\",\r\n        });\r\n        return;\r\n      }\r\n      axios\r\n        .post(`${this.baseURL}cart/add?token=${this.token}`, {\r\n          productId: productId,\r\n          quantity: this.quantity,\r\n        })\r\n        .then(\r\n          (response) => {\r\n            if (response.status == 201) {\r\n              swal({\r\n                text: \"Product Added to the cart!\",\r\n                icon: \"success\",\r\n                closeOnClickOutside: false,\r\n              });\r\n              // refresh nav bar\r\n              this.$emit(\"fetchData\");\r\n            }\r\n          },\r\n          (error) => {\r\n            console.log(error);\r\n          }\r\n        );\r\n    },\r\n\r\n    listCartItems() {\r\n      axios.get(`${this.baseURL}cart/?token=${this.token}`).then(\r\n        (response) => {\r\n          if (response.status === 200) {\r\n            this.$router.push(\"/cart\");\r\n          }\r\n        },\r\n        (error) => {\r\n          console.log(error);\r\n        }\r\n      );\r\n    },\r\n  },\r\n  mounted() {\r\n    this.id = this.$route.params.id;\r\n    this.product = this.products.find((product) => product.id == this.id);\r\n    this.category = this.categories.find(\r\n      (category) => category.id == this.product.categoryId\r\n    );\r\n    this.token = localStorage.getItem(\"token\");\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.category {\r\n  font-weight: 400;\r\n}\r\n\r\n/* Chrome, Safari, Edge, Opera */\r\ninput::-webkit-outer-spin-button,\r\ninput::-webkit-inner-spin-button {\r\n  -webkit-appearance: none;\r\n  margin: 0;\r\n}\r\n\r\n/* Firefox */\r\ninput[type=\"number\"] {\r\n  -moz-appearance: textfield;\r\n}\r\n\r\n#add-to-cart-button {\r\n  background-color: #febd69;\r\n}\r\n\r\n#wishlist-button {\r\n  background-color: #b9b9b9;\r\n  border-radius: 0;\r\n}\r\n\r\n#show-cart-button {\r\n  background-color: #131921;\r\n  color: white;\r\n  border-radius: 0;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}