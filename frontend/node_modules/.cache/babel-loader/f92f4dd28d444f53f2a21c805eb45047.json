ADnD,KAGE,CALJ,CASI,EAHJ,WAGI,EAHE,YAAA,CAEL,GAFK,EAEL;AAFQ,MAAA,IAAI,EAAC,GAEb;AAFiB,MAAA,KAAK,EAAC,YAEvB;AAFqC,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,UAAA,CAAW,QAAQ,CAAC,EAApB,CAAF;AAAA;AAE1C,KAFK,EACH,kBADG,EACa;AAAA;AADb,MACa,CAAA,SAAA,CADb,CAGF,CAtBN,CAuBI,CAxBN,C,EAyBA,W,EACA,W,CA5CF,C;GA6CM,CA7CN,E;;AAAA,GA2DI,GAZJ,mBAAA,CAAA,uBAAA,CAYI,EAXJ,YAAA,CAUM,KAVN,EAAA,WAAA,EAUM,CATJ,YAAA,CAAkD,IAAlD,EAAkD,IAAlD,EAAI,eAAe,gBAAA,CAAG,KAAA,CAAA,SAAA,CAAU,OAAV,CAAiB,CAAjB,CAAH,CAAnB,EAAuC;AAAA;AAAvC,GASI,EARJ,YAAA,CAOS,QAPT,EAOS;AANN,IAAA,QAAQ,EAAE,QAAA,CAAA,UAAA,EAMJ;AALP,IAAA,IAAI,EAAC,QAKE;AAJP,IAAA,KAAK,EAAC,yBAIC;AAHN,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,QAAA,IAAA,QAAA,CAAA,QAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAGC,GAPT,EAKC,gBALD,EAOA;AAAA;AAPA,IAOA,CAAA,UAAA,CAPA,CAQI,CAVN,CAWI,CAlEN,C","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"row\">\r\n      <div class=\"col-12 text-center\">\r\n        <h3 class=\"pt-3\">Shopping Cart</h3>\r\n      </div>\r\n    </div>\r\n    <!--    loop over all the cart items and display one by one-->\r\n    <div\r\n      v-for=\"cartItem in cartItems\"\r\n      :key=\"cartItem.menu.id\"\r\n      class=\"row mt-2 pt-3 justify-content-around\"\r\n    >\r\n      <div class=\"col-2\"></div>\r\n      <!-- display image -->\r\n      <div class=\"col-md-3 embed-responsive embed-responsive-16by9\">\r\n        <router-link\r\n          :to=\"{ name: 'ShowDetails', params: { id: cartItem.menu.id } }\"\r\n        >\r\n          <img\r\n            v-bind:src=\"cartItem.menu.image\"\r\n            class=\"w-100 card-img-top embed-responsive-item\"\r\n          />\r\n        </router-link>\r\n      </div>\r\n      <!-- display menu name, quantity and price -->\r\n      <div class=\"col-md-5 px-3\">\r\n        <div class=\"card-block px-3\">\r\n          <h6 class=\"card-title\">\r\n            <router-link\r\n              :to=\"{ name: 'ShowDetails', params: { id: cartItem.menu.id } }\"\r\n              >{{ cartItem.menu.name }}\r\n            </router-link>\r\n          </h6>\r\n          <p id=\"item-price\" class=\"mb-0 font-weight-bold\">\r\n            &#165; {{ cartItem.menu.price }} per unit\r\n          </p>\r\n          <p id=\"item-quantity\" class=\"mb-0\">\r\n            Quantity :{{ cartItem.quantity }}\r\n          </p>\r\n          <p id=\"item-total-price\" class=\"mb-0\">\r\n            Total Price:\r\n            <span class=\"font-weight-bold\">\r\n              &#165; {{ cartItem.menu.price * cartItem.quantity }}</span\r\n            >\r\n          </p>\r\n          <br /><a href=\"#\" class=\"text-right\" @click=\"deleteItem(cartItem.id)\"\r\n            >Remove From Cart</a\r\n          >\r\n        </div>\r\n      </div>\r\n      <div class=\"col-2\"></div>\r\n      <div class=\"col-12\"><hr style=\"width: 70%\" /></div>\r\n    </div>\r\n\r\n    <!-- display total price -->\r\n    <div class=\"total-cost pt-2 text-right total\">\r\n      <h5>Total : &#165; {{ totalcost.toFixed(2) }}</h5>\r\n      <button\r\n        :disabled=\"isDisabled()\"\r\n        type=\"button\"\r\n        class=\"btn btn-primary confirm\"\r\n        @click=\"checkout\"\r\n      >\r\n        Make Payment\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n<script>\r\nconst axios = require(\"axios\");\r\nexport default {\r\n  data() {\r\n    return {\r\n      cartItems: [],\r\n      token: null,\r\n      totalcost: 0,\r\n    };\r\n  },\r\n  name: \"Cart\",\r\n  props: [\"baseURL\"],\r\n  methods: {\r\n    isDisabled() {\r\n      if (this.cartItems.length === 0) {\r\n        return true;\r\n      }\r\n      return false;\r\n    },\r\n    // fetch all the items in cart\r\n    listCartItems() {\r\n      axios.get(`${this.baseURL}cart/?token=${this.token}`).then(\r\n        (response) => {\r\n          if (response.status == 200) {\r\n            const result = response.data;\r\n            // store cartitems and total price in two variables\r\n            this.cartItems = result.cartItems;\r\n            this.totalcost = result.totalCost;\r\n          }\r\n        },\r\n        (error) => {\r\n          console.log(error);\r\n        }\r\n      );\r\n    },\r\n    // go to checkout page\r\n    checkout() {\r\n      this.$router.push({ name: \"Checkout\" });\r\n    },\r\n    deleteItem(itemId) {\r\n      axios\r\n        .delete(`${this.baseURL}cart/delete/${itemId}/?token=${this.token} `)\r\n        .then(\r\n          (response) => {\r\n            if (response.status == 200) {\r\n              this.$router.go(0);\r\n            }\r\n            this.$emit(\"fetchData\");\r\n          },\r\n          (error) => {\r\n            console.log(error);\r\n          }\r\n        );\r\n    },\r\n    showDetails(menuId) {\r\n      this.$router.push({\r\n        name: \"ShowDetails\",\r\n        params: { id: menuId },\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.token = localStorage.getItem(\"token\");\r\n    this.listCartItems();\r\n  },\r\n};\r\n</script>\r\n  \r\n  <style scoped>\r\nh4,\r\nh5 {\r\n  font-family: \"Roboto\", sans-serif;\r\n  color: #484848;\r\n  font-weight: 700;\r\n}\r\n\r\n.embed-responsive .card-img-top {\r\n  object-fit: cover;\r\n}\r\n\r\n#item-price {\r\n  color: #232f3e;\r\n}\r\n\r\n#item-quantity {\r\n  float: left;\r\n}\r\n\r\n#item-total-price {\r\n  float: right;\r\n}\r\n\r\n.total-cost h5 {\r\n  margin-right: 20px;\r\n  padding-bottom: 5px;\r\n}\r\n\r\n.confirm {\r\n  font-weight: bold;\r\n  font-size: larger;\r\n}\r\n\r\n.total {\r\n  padding-right: 15%;\r\n}\r\n</style>\r\n  "],"sourceRoot":""}]}