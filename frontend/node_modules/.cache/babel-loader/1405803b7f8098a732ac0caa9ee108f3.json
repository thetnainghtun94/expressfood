SAAK,KAAL,GAAa,YAAY,CAAC,OAAb,CAAqB,OAArB,CAAb;AACA,SAAK,QAAL;AACD;AAnCY,CAAf","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"row\">\r\n      <div class=\"col-12 text-center\">\r\n        <h4 class=\"pt-3\">Order Id: {{ orderID }}</h4>\r\n      </div>\r\n    </div>\r\n\r\n    <div\r\n      v-for=\"(orderItem, index) in orderItems\"\r\n      :key=\"index\"\r\n      class=\"row mt-2 pt-3 justify-content-around\"\r\n    >\r\n      <div class=\"col-1\"></div>\r\n      <div class=\"col-md-3 embed-responsive embed-responsive-16by9\">\r\n        <img\r\n          :src=\"orderItem.product.imageURL\"\r\n          class=\"w-100 card-img-top embed-responsive-item\"\r\n        />\r\n        <hr />\r\n      </div>\r\n\r\n      <div class=\"col-md-5 px-3\">\r\n        <div class=\"card-block px-3\">\r\n          <h6 class=\"card-title\">{{ orderItem.product.name }}</h6>\r\n          <p id=\"item-price\" class=\"mb-0 font-weight-bold\">\r\n            ${{ orderItem.product.price }} per unit\r\n          </p>\r\n          <p id=\"item-quantity\" class=\"mb-0\">\r\n            Quantity : {{ orderItem.quantity }}\r\n          </p>\r\n          <p id=\"item-total-price\" class=\"mb-0\">\r\n            Total Price : $<span class=\"font-weight-bold\">{{\r\n              orderItem.price * orderItem.quantity\r\n            }}</span>\r\n          </p>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-1\"></div>\r\n    </div>\r\n\r\n    <div class=\"total-cost pt-2 text-right\">\r\n      <h5>Total Cost : $ {{ order.totalPrice }}</h5>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"OrderItems\",\r\n  props: [\"orderID\", \"baseURL\"],\r\n\r\n  data() {\r\n    return {\r\n      orderItems: [],\r\n      order: {},\r\n      token: null,\r\n      orderID: 0,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    getOrder() {\r\n      axios\r\n        .get(`${this.baseURL}order/${this.orderID}?token=${this.token}`)\r\n        .then(\r\n          (response) => {\r\n            if (response.status === 200) {\r\n              this.order = response.data;\r\n              this.orderItems = this.order.orderItems;\r\n            }\r\n          },\r\n          (err) => {\r\n            console.log(err);\r\n          }\r\n        );\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    this.orderID = this.$route.params.id;\r\n    this.token = localStorage.getItem(\"token\");\r\n    this.getOrder();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nh4 {\r\n  font-family: \"Roboto\", sans-serif;\r\n  color: #484848;\r\n  font-weight: 700;\r\n}\r\n\r\n.embed-responsive .card-img-top {\r\n  object-fit: cover;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}