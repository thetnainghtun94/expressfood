,KAAK,EAAC;;;AACJ,EAAA,KAAK,EAAC;;;AACL,EAAA,KAAK,EAAC;;;AAGP,EAAA,KAAK,EAAC;;;;;;AACN,EAAA,EAAE,EAAC,Y;AAAa,EAAA,KAAK,EAAC;;;AACtB,EAAA,EAAE,EAAC;;;+BAGV,YAAA,CAAyB,KAAzB,EAAyB;AAApB,EAAA,KAAK,EAAC;AAAc,CAAzB,EAAkB,IAAlB,EAAkB,CAAA;AAAA;AAAlB,C;;+BACA,YAAA,CAA8B,KAA9B,EAA8B;AAAzB,EAAA,KAAK,EAAC;AAAmB,CAA9B,EAAmB,C,aAAC,YAAA,CAAI,IAAJ,CAAD,CAAnB,E;;AAAA,C;;;;;;;uBAxBJ,YAAA,CA0BM,KA1BN,EAAA,UAAA,EA0BM,CAzBJ,UAyBI,EApBJ,mBAAA,CAAA,iCAAA,CAoBI,G,kBAnBJ,YAAA,CAkBM,SAlBN,EAkBM,IAlBN,EAkBM,WAAA,CAlBe,KAAA,CAAA,SAkBf,EAlBwB,UAAlB,KAAkB,EAAb;yBAAjB,YAAA,CAkBM,KAlBN,EAkBM;AAlB2B,MAAA,GAAG,EAAE,KAAK,CAAC,GAkBtC;AAlB2C,MAAA,KAAK,EAAC;AAkBjD,KAlBN,E,CACE,U,EACA,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,mBAAA,CAAA,uCAAA,CAEI,EADJ,YAAA,CAAkF,KAAlF,EAAkF;AAAtE,MAAA,GAAG,EAAE,KAAK,CAAC,QAA2D;AAAjD,MAAA,KAAK,EAAC;AAA2C,KAAlF,E,IAAA,E;;AAAA,M,OAAA,CACI,CAHN,C,EAIA,YAAA,CASM,KATN,EAAA,UAAA,EASM,CARJ,YAAA,CAOM,KAPN,EAAA,UAAA,EAOM,CANJ,YAAA,CAEK,IAFL,EAAA,UAAA,EAEK,CADH,YAAA,CAAiF,sBAAjF,EAAiF;AAA7D,MAAA,EAAE,EAAA,YAAY,KAAK,CAAC;AAAyC,KAAjF,E;uBAA4C;AAAA,eAAW,C,iBAAX,gBAAW,gBAAA,CAAE,KAAK,CAAC,EAAR,C,EAAU;AAAA;SAAV,CAAX;AAAA,O;;;;KAA5C,E;;AAAA,M,MAAA,CACG,CAFL,CAMI,EAHJ,YAAA,CAAuF,GAAvF,EAAA,UAAA,EAAuF,C,kCAArE,KAAK,CAAC,U,IAAY,O,EAAK;AAAA;KAA8C,EAAlC,KAAK,CAAC,UAAN,GAAgB,C,kBAA5B,YAAA,CAA0C,MAA1C,EAA0C,UAA1C,EAAkC,GAAlC,C,qCAA8C,CAAvF,CAGI,EAFJ,YAAA,CAAuF,GAAvF,EAAA,WAAA,EAAiD,oBAAe,gBAAA,CAAE,KAAK,CAAC,SAAR,CAAhE,EAAiF;AAAA;AAAjF,KAEI,EADJ,YAAA,CAA6D,GAA7D,EAAA,WAAA,EAAyB,kBAAa,gBAAA,CAAE,KAAK,CAAC,SAAR,CAAtC,EAAuD;AAAA;AAAvD,KACI,CAPN,CAQI,CATN,C,EAUA,W,EACA,W,CAjBF,C;GAkBM,CAlBN,E;;AAAA,GAmBI,EA1BN,C","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"row\">\r\n      <div class=\"col-12 text-center\">\r\n        <h4 class=\"pt-3\">Your Orders</h4>\r\n      </div>\r\n    </div>\r\n    <!--        for each order display -->\r\n    <div v-for=\"order in orderList\" :key=\"order.pid\" class=\"row mt-2 pt-3 justify-content-around\">\r\n      <div class=\"col-2\"></div>\r\n      <div class=\"col-md-3 embed-responsive embed-responsive-16by9\">\r\n        <!--                display image in left-->\r\n        <img v-bind:src=\"order.imageURL\" class=\"w-100 card-img-top embed-responsive-item\">\r\n      </div>\r\n      <div class=\"col-md-5 px-3\">\r\n        <div class=\"card-block px-3\">\r\n          <h6 class=\"card-title\">\r\n            <router-link v-bind:to=\"'/order/'+order.id\">Order No : {{order.id}}</router-link>\r\n          </h6>\r\n          <p class=\"mb-0\">{{order.totalItems}} item<span v-if=\"order.totalItems > 1\">s</span></p>\r\n          <p id=\"item-price\" class=\"mb-0 font-weight-bold\">Total Cost : $ {{order.totalCost}}</p>\r\n          <p id=\"item-total-price\">Ordered on : {{order.orderdate}}</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-2\"></div>\r\n      <div class=\"col-12\"><hr></div>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\n  const axios = require('axios')\r\n  export default {\r\n\r\n    data() {\r\n      return {\r\n        token: null,\r\n        orderList : []\r\n      }\r\n    },\r\n    props:[\"baseURL\"],\r\n    name: 'Order',\r\n    methods: {\r\n      // list of order histories\r\n      listOrders(){\r\n        axios.get(`${this.baseURL}order/?token=${this.token}`)\r\n          .then((response) => {\r\n              if(response.status==200){\r\n                this.orders = response.data\r\n                // for each order populate orderList\r\n                this.orders.forEach((order) => {\r\n                  this.orderList.push({\r\n                    id: order.id,\r\n                    totalCost: order.totalPrice,\r\n                    // get short date\r\n                    orderdate: order.createdDate.substring(0,10),\r\n                    // get image of the first orderItem of the order\r\n                    imageURL: order.orderItems[0].product.imageURL,\r\n                    // get total items\r\n                    totalItems: order.orderItems.length\r\n                  })\r\n                })\r\n              }\r\n            },\r\n            (error)=>{\r\n              console.log(error)\r\n            });\r\n      },\r\n    },\r\n    mounted() {\r\n      this.token = localStorage.getItem(\"token\");\r\n      this.listOrders();\r\n    },\r\n  };\r\n\r\n</script>\r\n\r\n<style scoped>\r\n  h4, h5 {\r\n    font-family: 'Roboto', sans-serif;\r\n    color: #484848;\r\n    font-weight: 700;\r\n  }\r\n\r\n  .embed-responsive .card-img-top {\r\n    object-fit: cover;\r\n  }\r\n</style>\r\n"],"sourceRoot":""}]}