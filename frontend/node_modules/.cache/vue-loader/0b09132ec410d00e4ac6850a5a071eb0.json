AAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;AACnB","file":"C:/Users/User/Desktop/Express Food/food_vuejs/src/views/Orders/Order.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"row\">\r\n      <div class=\"col-12 text-center\">\r\n        <h4 class=\"pt-3\">Your Orders</h4>\r\n      </div>\r\n    </div>\r\n    <!--        for each order display -->\r\n    <div\r\n      v-for=\"order in orderList\"\r\n      :key=\"order.pid\"\r\n      class=\"row mt-2 pt-3 justify-content-around\"\r\n    >\r\n      <div class=\"col-2\"></div>\r\n      <div class=\"col-md-3 embed-responsive embed-responsive-16by9\">\r\n        <!--                display image in left-->\r\n        <img\r\n          v-bind:src=\"order.image\"\r\n          class=\"w-100 card-img-top embed-responsive-item\"\r\n        />\r\n      </div>\r\n      <div class=\"col-md-5 px-3\">\r\n        <div class=\"card-block px-3\">\r\n          <h6 class=\"card-title\">\r\n            <router-link v-bind:to=\"'/order/' + order.id\"\r\n              >Order No : {{ order.id }}</router-link\r\n            >\r\n          </h6>\r\n          <p class=\"mb-0\">\r\n            {{ order.totalItems }} item<span v-if=\"order.totalItems > 1\"\r\n              >s</span\r\n            >\r\n          </p>\r\n          <p id=\"item-price\" class=\"mb-0 font-weight-bold\">\r\n            Total Cost : &#165; {{ order.totalCost }}\r\n          </p>\r\n          <p id=\"item-total-price\">Ordered on : {{ order.orderdate }}</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-2\"></div>\r\n      <div class=\"col-12\"><hr style=\"width: 70%\" /></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n  <script>\r\nconst axios = require(\"axios\");\r\nexport default {\r\n  data() {\r\n    return {\r\n      token: null,\r\n      orderList: [],\r\n    };\r\n  },\r\n  props: [\"baseURL\"],\r\n  name: \"Order\",\r\n  methods: {\r\n    // list of order histories\r\n    listOrders() {\r\n      axios.get(`${this.baseURL}order/?token=${this.token}`).then(\r\n        (response) => {\r\n          if (response.status == 200) {\r\n            this.orders = response.data;\r\n            // for each order populate orderList\r\n            this.orders.forEach((order) => {\r\n              this.orderList.push({\r\n                id: order.id,\r\n                totalCost: order.totalPrice,\r\n                // get short date\r\n                orderdate: order.createdDate.substring(0, 10),\r\n                // get image of the first orderItem of the order\r\n                image: order.orderItems[0].menu.image,\r\n                // get total items\r\n                totalItems: order.orderItems.length,\r\n              });\r\n            });\r\n          }\r\n        },\r\n        (error) => {\r\n          console.log(error);\r\n        }\r\n      );\r\n    },\r\n  },\r\n  mounted() {\r\n    this.token = localStorage.getItem(\"token\");\r\n    this.listOrders();\r\n  },\r\n};\r\n</script>\r\n  \r\n  <style scoped>\r\nh4,\r\nh5 {\r\n  font-family: \"Roboto\", sans-serif;\r\n  color: #484848;\r\n  font-weight: 700;\r\n}\r\n\r\n.embed-responsive .card-img-top {\r\n  object-fit: cover;\r\n}\r\n</style>\r\n  "]}]}