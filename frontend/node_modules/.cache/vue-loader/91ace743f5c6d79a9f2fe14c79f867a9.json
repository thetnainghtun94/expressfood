ood_vuejs/src/views/Menu/EditMenu.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div class=\"container\" style=\"font-family: sans-serif\">\r\n    <div class=\"row\">\r\n      <div class=\"col-12 text-center\">\r\n        <h4 class=\"pt-3\">Edit Menu</h4>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-3\"></div>\r\n      <div class=\"col-md-6 px-5 px-md-0\">\r\n        <form v-if=\"menu\">\r\n          <div class=\"form-group\">\r\n            <label>Category</label>\r\n            <select class=\"form-control\" v-model=\"menu.categoryId\" required>\r\n              <option\r\n                v-for=\"category of categories\"\r\n                :key=\"category.id\"\r\n                :value=\"category.id\"\r\n              >\r\n                {{ category.categoryName }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label>Name</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              v-model=\"menu.name\"\r\n              required\r\n            />\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label>Description</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              v-model=\"menu.description\"\r\n              required\r\n            />\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label>Image</label>\r\n            <input\r\n              type=\"url\"\r\n              class=\"form-control\"\r\n              v-model=\"menu.image\"\r\n              required\r\n            />\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label>Price</label>\r\n            <input\r\n              type=\"number\"\r\n              class=\"form-control\"\r\n              v-model=\"menu.price\"\r\n              required\r\n            />\r\n          </div>\r\n          <div class=\"text-center\">\r\n            <button type=\"button\" class=\"btn btn-warning\" @click=\"editMenu\">\r\n              Submit\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n      <div class=\"col-3\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      menu: null,\r\n    };\r\n  },\r\n  props: [\"baseURL\", \"menus\", \"categories\"],\r\n  methods: {\r\n    async editMenu() {\r\n      axios\r\n        .post(this.baseURL + \"menu/update/\" + this.id, this.menu)\r\n        .then((res) => {\r\n          //sending the event to parent to handle\r\n          this.$emit(\"fetchData\");\r\n          this.$router.push({ name: \"AdminMenu\" });\r\n          swal({\r\n            text: \"Menu Updated Successfully!\",\r\n            icon: \"success\",\r\n            closeOnClickOutside: false,\r\n          });\r\n        })\r\n        .catch((err) => console.log(\"err\", err));\r\n    },\r\n  },\r\n  mounted() {\r\n    if (!localStorage.getItem(\"token\")) {\r\n      this.$router.push({ name: \"Signin\" });\r\n      return;\r\n    }\r\n    this.id = this.$route.params.id;\r\n    this.menu = this.menus.find((menu) => menu.id == this.id);\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nh4 {\r\n  font-family: \"Roboto\", sans-serif;\r\n  color: #484848;\r\n  font-weight: 700;\r\n}\r\n</style>\r\n"]}]}